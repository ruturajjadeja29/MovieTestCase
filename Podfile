# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'Movie' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

# Pods for Movie
    # Web Service
    pod 'Alamofire'

    # Image Loading
    pod 'Kingfisher'
    
    # Reactive
    pod 'RxSwift'
    pod 'RxCocoa'
    
    # Reactive Coredata
    pod 'RxCoreData'
    pod 'RxDataSources'

  target 'MovieTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MovieUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
